algoritmo "Somador"

var
N1,N2:Inteiro

Procedimento Soma(A,B:Inteiro)
Inicio
      Escreval("Recebi o valor ",A)
      Escreval("Recebi o valor ",B)
      Escreval("A soma entre ",N1," e ",N2," é ", A + B)
FimProcedimento

inicio
Escreval("-------------------------")
Escreval("           SOMA          ")
Escreval("-------------------------")
N1<-8
N2<-5
Soma(N1,N2)
fimalgoritmo
